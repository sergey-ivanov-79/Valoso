            {"type": "font", "name": "Global Font", "find": "'Lato'"},

            {"type": "color", "find": "#fea906", "name": "Accent Color"},
            {"type": "color", "find": "#505051", "name": "General Text"},
            {"type": "color", "find": "#feffff", "name": "Button Text"},
            {"type": "color", "find": "#000001", "name": "Header Background"},
            {"type": "color", "find": "#cdcccc", "name": "Banner Background (No Image)"},
